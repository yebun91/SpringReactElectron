<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.demo.mybatis.dao.MemberDao">
    <select id="getAllDataList" resultType="com.example.demo.mybatis.dto.MemberDto">
        select * from member;
    </select>
    <select id="getMemberChat" resultType="com.example.demo.mybatis.dto.ChatDto">
        select * from chat_list where to_id = ${id};
    </select>
</mapper>